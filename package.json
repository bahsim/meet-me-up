{
  "name": "meet-me-up",
  "version": "1.0.0",
  "description": "dating service",
  "main": "index.js",
  "scripts": {
    "PRODUCTION CONFIGURATION": "//",
    "start": "npm run clean && npm run build && npm run serve",
    "build": "npm run build:vendor && npm run build:app",
    "build:app": "cross-env NODE_ENV=production webpack -p --config webpack.config.js --progress",
    "build:vendor": "cross-env NODE_ENV=production webpack -p --config vendor.webpack.config.js --progress",
    "serve": "cross-env NODE_ENV=production pm2 start server.js --no-daemon",
    "healthcheck": "cross-env NODE_ENV=production pm2 show server | grep online",
    "DEVELOPMENT CONFIGURATION": "//",
    "dev": "npm run clean && concurrently --kill-others \"npm run build:dev\" \"npm run serve:dev\"",
    "build:dev": "npm run build:vendor:dev && npm run build:app:dev",
    "build:app:dev": "cross-env NODE_ENV=development webpack --mode=development --config webpack.config.js --progress --watch",
    "build:vendor:dev": "cross-env NODE_ENV=development webpack  --mode=development --config vendor.webpack.config.js --progress",
    "serve:dev": "cross-env NODE_ENV=development nodemon --watch server.js --exec node server.js",
    "TOOLS & CONFIGURATIONS": "//",
    "clean": "rimraf dist/",
    "test": "npm run lint",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "prettier": "prettier --bracket-spacing --list-different --single-quote --trailing-comma all --write \"src/**/*.js\""
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "engines": {
    "node": ">=8.9.2"
  },
  "dependencies": {
    "@date-io/moment": "^1.3.11",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.2.6",
    "@material-ui/styles": "^4.3.3",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "connect-redis": "3.4.0",
    "cookie-parser": "^1.4.4",
    "eslint-plugin-react-hooks": "^2.0.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "offline-plugin": "^5.0.7",
    "pm2": "^3.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-image-lightbox": "^5.1.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "reapop": "^2.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.6",
    "redux-form-input-masks": "^2.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cypress": "^3.4.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "node-sass": "^7.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-notifier": "^1.8.0"
  }
}
